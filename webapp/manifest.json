{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "ysc.websocket",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"init_data": {
				"uri": "model/init_data.json",
				"type": "JSON"
			},
			"salesNorthwind": {
				"uri": "model/salesNorthwind.json",
				"type": "JSON"
			},
			"northwind": {
				"uri": "services.odata.org/Northwind/Northwind.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {

		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "ysc.websocket.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ysc.websocket.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "init_data"
			},
			"salesNorthwind": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "salesNorthwind"
			},
			"northwind": {
				"dataSource": "northwind"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ysc.websocket.view",
				"controlId": "idAppControl",
				"viewType": "XML",
				"transition": "slide"
			},
			"routes": [{
				"name": "master",
				"pattern": "",
				"target": ["Master"]
			}, {
				"name": "salesByCountry",
				"pattern": "SalesCountry/{Year}",
				"target": ["Master", "SalesByCountry"]
			}, {
				"name": "salesByCategory",
				"pattern": "SalesCategory/{Year}/:CategoryName:",
				"target": ["Master", "SalesByCategory"]
			}, {
				"name": "salesByProduct",
				"pattern": "SalesProduct/{Year}",
				"target": ["Master", "SalesByProduct"]
			}],
			"targets": {
				"Master": {
					"viewName": "Master",
					"controlAggregation": "masterPages",
					"title": {
						"parts": ["mainPageTitle"],
						"formatter": "jQuery.sap.formatMessage"
					},
					"viewLevel": 0
				},
				"SalesByCountry": {
					"viewName": "SalesByCountry",
					"controlAggregation": "detailPages",
					"title": {
						"parts": ["detailPageTitle"],
						"formatter": "jQuery.sap.formatMessage"
					},
					"viewLevel": 1
				},
				"SalesByCategory": {
					"viewName": "SalesByCategory",
					"viewId": "idSalesByCategory",
					"controlAggregation": "detailPages",
					"title": {
						"parts": ["detailPageTitle"],
						"formatter": "jQuery.sap.formatMessage"
					},
					"viewLevel": 1
				},
				"SalesByProduct": {
					"viewName": "SalesByProduct",
					"viewId": "idSalesByProduct",
					"controlAggregation": "detailPages",
					"title": {
						"parts": ["detailPageTitle"],
						"formatter": "jQuery.sap.formatMessage"
					},
					"viewLevel": 1
				},
				"SOCKETSENDER": {
					"viewName": "SocketSender",
					"viewId": "idSocketSender"
				}
			}
		}
	}
}