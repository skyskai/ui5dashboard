													indicator="{localModel>trend}"
													scale="{localModel>scale}" />
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
								</layout:HorizontalLayout>
							</IconTabFilter>
							<IconTabSeparator icon="sap-icon://open-command-field" />
							<IconTabFilter
							    icon="sap-icon://shipping-status"
								text="출하관리"
								count="58">
								<layout:HorizontalLayout allowWrapping="true" content="{localModel>/Tiles3}">
