<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"  xmlns:layout="sap.ui.layout" controllerName="ysc.websocket.controller.Launchpad"
	xmlns:html="http://www.w3.org/1999/xhtml">
	
			<Page showHeader="false">
				<content>
					<!--<layout:Grid defaultSpan="L2 M6 S6" content="{localModel>/Tiles}">-->
					<!--<Panel headerTex="영업">-->
					<IconTabBar
						class="sapUiResponsiveContentPadding">
						<items>
							<IconTabFilter
							    icon="sap-icon://travel-request"
								text="견적관리"
								count="65">
								<layout:HorizontalLayout allowWrapping="true" content="{localModel>/Tiles1}">
									<GenericTile
									header="{localModel>title}"
									subheader="{localModel>subtitle}"
									mode="{localModel>mode}"
									press="press"
									class="sapUiTinyMarginBegin sapUiTinyMarginTop">
									<tileContent>
										<TileContent unit="{localModel>unit}" footer="{localModel>footer}" footerColor="{localModel>footerColor}">
											<content>
												<NumericContent
													value="{localModel>kpivalue}"
													valueColor="{localModel>color}"
													indicator="{localModel>trend}"
													scale="{localModel>scale}" />
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>
								</layout:HorizontalLayout>
							</IconTabFilter>
							<IconTabSeparator icon="sap-icon://open-command-field" />
							<IconTabFilter
							    icon="sap-icon://order-status"
								text="주문관리"
								count="69" >
